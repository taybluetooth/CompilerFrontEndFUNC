method pow(x,y) vars i, res,w
begin

	res := x(da,da(1,2,m(1,1)),1);
	i := 2;
	if eq(x,32) then 
		write 1;
		read a;
	else
		b := 11;
	endif;
	while less(i,y)
	begin
		res := times(res,x);
		i := plus(i,1);
        endwhile;
	write res;
	return res;

endmethod

method main() vars a, b, x
begin

	a := 5; 
	b := 2;
	x := pow(b,a);
	if eq(x,32)
		 then write 1; 
	else 
		write 0;
	endif; 
endmethod
